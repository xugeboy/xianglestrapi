    "width": {
      "type": "string"
    },
    "working_load_limit": {
      "type": "string"
    },
    "material": {
      "type": "string"
    },
    "webbing_break_strength": {
      "type": "string"
    },
    "grade": {
      "type": "string"
    },
    "ratchet_handle": {
      "type": "string"
    },
    "finish": {
      "type": "string"
    },
    "product_weight": {
      "type": "string"
    },
    "code": {
      "type": "string"
    },
